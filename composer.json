{
    "name": "wedesignit/laravel-performance",
    "description": "Optimize your code even before it hits production",
    "keywords": [
        "WeDesignIt",
        "laravel-performance",
        "eager loading",
        "slow query logging"
    ],
    "homepage": "https://github.com/WeDesignIt/laravel-performance",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick van Kouteren",
            "email": "p.vankouteren@wedesignit.nl",
            "homepage": "https://www.wedesignit.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/database": "^8.19|^9.0",
        "illuminate/support": "^8.19|^9.0"
    },
    "require-dev": {
        "laravel/framework": "^8.19|^9.0"
    },
    "autoload": {
        "psr-4": {
            "WeDesignIt\\LaravelPerformance\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "WeDesignIt\\LaravelPerformance\\Providers\\LaravelPerformanceServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
